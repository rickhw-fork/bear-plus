<div id="note-column">
  <div id="search-bar">
    <form id=search-form>
      <input type=text placeholder="Search Notes">
    </form>
    <%if (profile !== null && profileId === userId) { %>
      <button id="new-note">+</button>
    <% } %>
  </div>
  <div id="notes-container">
    <div id="notes">
    </div>
  </div>
</div>
